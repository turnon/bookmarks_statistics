<div id="tags" style="position:fixed;right:100px;top:0px">
  <% _.each(subs, function(sub){ %>
    <ul>
      <li>
        <a href="#<%= sub.replace(/\s/g,'_')%>"><%= sub%></a>
      </li>
    </ul>
  <% }); %>
</div>
<div id="reports">
  <% _.each(subs, function(sub){ %>
    <div id="<%= sub.replace(/\s/g,'_') %>"></div>
  <% }); %>
</div>